<div class="col-lg-10 col-lg-offset-1">
 
    <h1><i class="fa fa-users"></i> User Administration </h1>
 
    <div class="table-responsive">
        <table class="table table-bordered table-striped">
 
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Username</th>
                </tr>
            </thead>
 
            <tbody>
               <% @user.each do |user|%>
					<tr>
						<td><%= user.id %></td>
						<td><%= user.email %></td>
					</tr>
				<% end %>
            </tbody>
 
        </table>
    </div>
 
</div>


